{
  "name": "fis3-lint-rich-stylelint",
  "version": "1.0.1",
  "description": "a based stylelint plugin for fis3",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoweiTang/fis3-lint-rich-stylelint.git"
  },
  "keywords": [
    "fis3",
    "lint",
    "stylelint",
    "nodejs",
    "javascript"
  ],
  "stylelintRules": {
    "indentation": [
      4,
      {
        "severity": "warning"
      }
    ],
    "max-empty-lines": [
      2,
      {
        "severity": "warning"
      }
    ],
    "block-no-empty": true,
    "comment-no-empty": true,
    "value-list-comma-space-after": [
      "always",
      {
        "severity": "warning"
      }
    ],
    "value-list-max-empty-lines": [
      0,
      {
        "severity": "warning"
      }
    ],
    "declaration-block-semicolon-space-after": [
      "always-single-line",
      {
        "severity": "warning"
      }
    ],
    "block-closing-brace-empty-line-before": [
      "never",
      {
        "severity": "warning"
      }
    ],
    "block-opening-brace-newline-after": "always-multi-line",
    "block-opening-brace-space-before": [
      "always",
      {
        "severity": "warning"
      }
    ],
    "declaration-colon-space-after": "always",
    "rule-empty-line-before": [
      "always-multi-line",
      {
        "severity": "warning",
        "except": [
          "first-nested"
        ],
        "ignore": [
          "after-comment"
        ]
      }
    ],
    "function-calc-no-unspaced-operator": [
      true,
      {
        "severity": "warning"
      }
    ],
    "declaration-no-important": true,
    "media-feature-name-no-unknown": true,
    "no-duplicate-at-import-rules": true,
    "no-unknown-animations": true,
    "selector-max-compound-selectors": [
      3,
      {
        "severity": "warning"
      }
    ],
    "selector-max-id": 1,
    "max-nesting-depth": [
      3,
      {
        "severity": "warning"
      }
    ],
    "color-no-invalid-hex": true,
    "color-named": [
      "never",
      {
        "severity": "warning"
      }
    ],
    "string-no-newline": true,
    "number-max-precision": [
      2,
      {
        "severity": "warning"
      }
    ],
    "unit-no-unknown": true,
    "property-no-unknown": [true, {
      "ignoreProperties": ["linear-gradient"]
    }],
    "selector-pseudo-class-no-unknown": true,
    "selector-pseudo-element-no-unknown": true,
    "declaration-block-no-duplicate-properties": [true, {
      "ignoreProperties": ["background"]
    }],
    "no-duplicate-selectors": true,
    "no-extra-semicolons": true,
    "declaration-block-trailing-semicolon": "always",
    "string-quotes": [
      "double",
      {
        "severity": "warning"
      }
    ],
    "value-keyword-case": "lower",
    "property-case": "lower"
  },
  "engines": {
    "node": ">= 4.7.0"
  },
  "author": "ice.tang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guoweiTang/fis3-lint-rich-stylelint/issues"
  },
  "homepage": "https://github.com/guoweiTang/fis3-lint-rich-stylelint#readme",
  "dependencies": {
    "stylelint": "^8.4.0"
  }
}